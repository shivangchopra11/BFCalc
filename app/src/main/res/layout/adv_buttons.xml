<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/adv">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#0000">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:background="#0000">
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="sin"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bsin"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"
                android:padding="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="cos"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bcos"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>

            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="tan"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/btan"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:background="#0000">
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="ln"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bln"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="log"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/blog"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="^"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bpow"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:background="#0000">
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="sinh"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bsinh"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="cosh"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bcosh"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="tanh"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/btanh"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:background="#0000">
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Ï€"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bpi"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="e"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bexp"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="%"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bperc"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:background="#0000">
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="("
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bbb"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text=")"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bbe"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="del"
                android:background="@color/colorPrimaryLight"
                android:id="@+id/bdel"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:minHeight="0dp"
                android:minWidth="0dp"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>